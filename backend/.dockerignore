# Ignore the virtual Enviroment
venv/
.venv/

# ignore python's temporary cache files
__pycache__/
*.pyc

# --- Local Database Files ---
# The container will use a production database, not our local SQLite files.
instance/
*.db
*.sqlite3

# Ignore folders created by code editors like VS Code.
.vscode/
.idea/
.DS_Store

# Ignore the cache folder generated by Pytest.
.pytest_cache/

# Never include the Git version control history in the Docker image.
.git/
.gitignore